<%= form_for :user, url: users_path do |f| %>
    <p>
      <%= f.label :nome %><br>
      <%= f.text_field :nome %>
    </p>

    <p>
      <%= f.label :email %><br>
      <%= f.email_field :email %>
    </p>

    <p>
      <%= f.label :password %><br>
      <%= f.password_field :password %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>